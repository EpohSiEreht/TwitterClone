<section ng-controller="UsersController">


  <form ng-submit="createTweet()">
    <input type="hidden" ng-model="newUser.name" name="name">
    <input type="hidden" ng-model="newUser.image" name="image">
    <input type="hidden" ng-model="newUser.username" name="username">
    <input type="text" ng-model="newUser.tweet" name="tweet"placeholder="What's happening?">
    <input type="submit">
  </form>

  <div ng-repeat="user in users">

    <img src={{user.image}}>
    <h3> {{user.name}}</h3>
    <h6> {{user.username}}</h6>
    <p>{{user.tweet}}</p>

  </div>

</section>
